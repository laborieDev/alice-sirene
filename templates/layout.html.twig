<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Alice Sirene{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body>
        <header>
            {% include 'Layout/header.html.twig' %}
        </header>
        <main class="{% block page_classes %}{% endblock %}">
            {% block content %}{% endblock %}
        </main>
        <footer>
            {% include 'Layout/footer.html.twig' %}
        </footer>
    </body>
</html>

