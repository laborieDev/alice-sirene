<div class="single-datas">
    <div class="category">
        <b>{{ etablissement.name }}</b>
        {% if etablissement.category != null %}
            ({{ etablissement.category }})
        {% endif %}
    </div>
    <div class="resume-datas">
        <i class="fa fa-info-circle" aria-hidden="true"></i>
        <span>SIRET : <b>{{ etablissement.siret }}</b></span>
        <span>SIREN : <b>{{ etablissement.siren }}</b></span>
        <span>NIC : <b>{{ etablissement.nic }}</b></span>
    </div>
    <div class="address">
        <i class="fa fa-map-marker" aria-hidden="true"></i>
        {{ etablissement.address }}
    </div>
    <div class="address">
        <i class="fa fa-calendar" aria-hidden="true"></i>
